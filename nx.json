{
  "npmScope": "portfolio",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "ma-studio": {
      "tags": []
    },
    "ma-studio-e2e": {
      "tags": [],
      "implicitDependencies": ["ma-studio"]
    },
    "portfolio": {
      "tags": []
    },
    "portfolio-e2e": {
      "tags": [],
      "implicitDependencies": ["portfolio"]
    },
    "shared-ng-ui": {
      "tags": ["scope:ng", "type:ui"]
    },
    "shared-svg": {
      "tags": []
    }
  }
}
